{
  "name": "aws-backend",
  "version": "1.0.0",
  "description": "AWS backend project",
  "scripts": {
    "lint-staged": "lint-staged",
    "prepare": "husky install",
    "typescript-compile": "tsc",
    "build": "webpack --config config/webpack.config.js",
    "test": "jest --config=jest.config.js",
    "integration-test": "./run-integration-test.sh",
    "lint": "eslint . --config config/.eslintrc.js --ignore-path config/.eslintignore",
    "eslint-autofix": "eslint . --fix  --config config/.eslintrc.js --ignore-path config/.eslintignore",
    "typecheck": "tsc --project tsconfig.dev.json",
    "render": "terrafirma render -c ./infrastructure/config.yaml"
  },
  "author": "George Lei",
  "license": "MIT",
  "devDependencies": {
    "@types/aws-lambda": "^8.10.136",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "eslint": "^8.57.0",
    "husky": "^9.0.11",
    "prettier": "^3.2.5",
    "ts-loader": "^9.5.1",
    "tsconfig-paths-webpack-plugin": "^4.1.0",
    "webpack": "^5.90.3",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "aws-lambda": "^1.0.7"
  }
}